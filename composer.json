{
  "name": "phpmx/phpmexico",
  "description": "Sitio web de PHP MÃ©xico",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "dms/meetup-api-client": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^2.2",
        "easycorp/easyadmin-bundle": "^2.3",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "google/recaptcha": "^1.2",
        "gravatarphp/gravatar": "^2.0",
        "jaybizzle/crawler-detect": "^1.2",
        "mattketmo/email-checker": "^1.5",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.18",
        "predis/predis": "^1.1",
        "presta/sitemap-bundle": "^1.7",
        "sensio/framework-extra-bundle": "^5.5",
        "snc/redis-bundle": "^2.1",
        "symfony/cache": "4.4.*",
        "symfony/config": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.9",
        "symfony/framework-bundle": "4.4.*",
        "symfony/mercure-bundle": "^0.1.1",
        "symfony/monolog-bundle": "^3.5",
        "symfony/security-csrf": "4.2.*",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/templating": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "wrapi/slack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "lint" : "./vendor/bin/php-cs-fixer fix --config=.php_cs.dist --verbose --show-progress=estimating",
        "lint:check" : "./vendor/bin/php-cs-fixer fix --config=.php_cs.dist --verbose --show-progress=estimating --dry-run"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.20",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/web-server-bundle": "4.4.*"
  }
}
